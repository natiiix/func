[stdio]

(func main (int argc, char** argv) int
    (puts [argv])

    (if (> argc 1)
        (loop (-= argc 1)
            (printf "%s\n" [argv argc])
        )
        (puts "Hello World!")
    )

    (printf "%d\n" (fact 10))

    (return 0)
)

(func fact (int n) int
    (if (<= n 0)
        (return 1)
        (return
            (* n
                (fact
                    (- n 1)
                )
            )
        )
    )
)
