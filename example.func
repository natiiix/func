(include stdio)

(func int main (int argc, char** argv)
    (if (> argc 1)
        (loop argc
            (printf "%s\n" argv[(- argc 1)])
            (-= argc 1)
        )
        (puts "Hello World!")
    )

    (printf "%d\n" (fact 10))

    (return 0)
)

(func int fact (int n)
    (if (<= n 0)
        (return 1)
        (return
            (* n
                (fact
                    (- n 1)
                )
            )
        )
    )
)
